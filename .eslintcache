, Nav } from 'react-bootstrap';\n\nclass App extends Component {\n  render() {\n    return (\n      <Router>\n        <div className=\"App\">\n          {/*Navigation Bar present across all components/pages displayed  */}\n          <Navbar expand=\"lg\" bg=\"dark\" variant=\"dark\">\n            <Navbar.Brand href=\"/\">My Home Library</Navbar.Brand>\n            <Nav className=\"mr-auto\">\n              {/*Links to each Component Page */}\n              <Nav.Link href=\"/\">Home</Nav.Link>\n              <Nav.Link href=\"/library\">Library</Nav.Link>\n              <Nav.Link href=\"/addBook\">Add Book</Nav.Link>\n            </Nav>\n          </Navbar>\n\n          {/*URLs to app pages linking to components */}\n          <Switch>\n            <Route path='/' component={Index} exact></Route>\n            <Route path='/library' component={Library} exact></Route>\n            <Route path='/addBook' component={AddBook} exact></Route>\n            <Route path='/updateBook/:id' component={UpdateBook} exact></Route>\n          </Switch>\n        </div>\n      </Router>\n    );\n  }\n\n}\n\nexport default App;\n","C:\\Users\\jrenn\\Desktop\\Year 4 2020 - 2021\\Data Representation and Querying\\Project\\my-home-library\\src\\components\\index.js",[],"C:\\Users\\jrenn\\Desktop\\Year 4 2020 - 2021\\Data Representation and Querying\\Project\\my-home-library\\src\\components\\library.js",[],"C:\\Users\\jrenn\\Desktop\\Year 4 2020 - 2021\\Data Representation and Querying\\Project\\my-home-library\\src\\components\\addBook.js",[],"C:\\Users\\jrenn\\Desktop\\Year 4 2020 - 2021\\Data Representation and Querying\\Project\\my-home-library\\src\\components\\books.js",[],"C:\\Users\\jrenn\\Desktop\\Year 4 2020 - 2021\\Data Representation and Querying\\Project\\my-home-library\\src\\components\\bookProfile.js",["44"],"import React from 'react';\r\n//Bootstrap import for the Card.\r\nimport Card from 'react-bootstrap/Card';\r\n//Bootstrap import for the Button\r\nimport Button from 'react-bootstrap/Button';\r\n//Importing Axois\r\nimport axois from 'axios';\r\n//Imported to change the URL and pass ID of current work\r\nimport {Link} from 'react-router-dom';\r\n//Import Style Sheet\r\nimport '../style/style.css';\r\n\r\nexport class BookProfile extends React.Component {\r\n\r\n    constructor(){\r\n        super();\r\n\r\n        this.DeleteBook = this.DeleteBook.bind(this);\r\n    }\r\n\r\n    //Delete Function\r\n    DeleteBook(e) {\r\n        //Stops Method getting Called on Load\r\n        e.preventDefault();\r\n        //Shows delete in the Console\r\n        console.log(\"Delete: \" + this.props.book._id);\r\n\r\n        //Deletes Book from the Server and Reloads Page\r\n        axois.delete(\"http://localhost:4000/api/books/\" + this.props.book._id)\r\n            .then(() => {\r\n                this.props.ReloadData();\r\n            })\r\n            .catch();\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <div>\r\n                {/*The content of each bookProfile is desplayed as requested, BookTitle, Author and Genre */}\r\n                <Card className=\"BookCard\">\r\n                    {/*Card Holds Title in the Card Header. Year and Poster are held in the Card Body  */}\r\n                    <Card.Header className=\"bTitle\"><h3>{this.props.book.bookTitle}</h3></Card.Header>\r\n                    <Card.Body>\r\n                        <blockquote className=\"blockquote mb-0\">\r\n                            <img className=\"CoverImg\" src={this.props.book.coverImg} width=\"250\" height=\"300\" alt=\"Book Cover\"/>\r\n                            <h5>Synopsis of Book</h5>\r\n                            <p>{this.props.book.synopsis}</p>\r\n                            <h5>Genre of Book</h5>\r\n                            <p>{this.props.book.genre}</p>\r\n                            <h5>Lent Out </h5>\r\n                            <p>{this.props.book.lent}</p>\r\n                            <footer className=\"blockquote-footer\">\r\n                                <h6>Author</h6>\r\n                                {this.props.book.author} \r\n                            </footer>\r\n                        </blockquote>\r\n                    </Card.Body>\r\n                    {/*Button to Delete Record*/}\r\n                    <Button className=\"bookBtn\" variant=\"danger\" onClick={this.DeleteBook}>Delete</Button>\r\n                    {/*Button to Upadte Record*/}\r\n                    <Link className=\"bookBtn\" to={\"/updateBook/\" + this.props.book._id} className=\"btn btn-dark\">Update</Link>\r\n                </Card>\r\n\r\n            </div>\r\n        );\r\n    }\r\n}","C:\\Users\\jrenn\\Desktop\\Year 4 2020 - 2021\\Data Representation and Querying\\Project\\my-home-library\\src\\components\\updateBook.js",[],{"ruleId":"45","replacedBy":"46"},{"ruleId":"47","replacedBy":"48"},{"ruleId":"49","severity":1,"message":"50","line":1,"column":8,"nodeType":"51","messageId":"52","endLine":1,"endColumn":12},{"ruleId":"53","severity":1,"message":"54","line":61,"column":89,"nodeType":"55","endLine":61,"endColumn":113},"no-native-reassign",["56"],"no-negated-in-lhs",["57"],"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","react/jsx-no-duplicate-props","No duplicate props allowed","JSXAttribute","no-global-assign","no-unsafe-negation"]